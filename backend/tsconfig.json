{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "baseUrl": "./",
        "paths": {
            "@config/*": ["src/config/*"],
            "@middlewares/*": ["src/middlewares/*"],
            "@routes/*": ["src/routes/*"],
            "@controllers/*": ["src/controllers/*"]
        },
        "outDir": "./dist",
    }
}
